#a
x = :welcome_to_ruby
p (x.start_with?("wel")) || (x.start_with?("to")) || (x.start_with?("zzz"))
#b
p (x.end_with?("by")) || (x.end_with?("zy"))
#c
p :welcome_to_rubz
#d
p x.to_s.gsub!(/[^0-9A-Za-z]/, '').length
#e
p x.capitalize()
p x
#f
p x.to_s
p x
#g
s = x.to_s
arr = s.split("")

p arr.map {|e| e.to_sym}
